<h1> Tasks </h1>
<h2> Your Tasks for Today: </h2>
  <ul>
    <% @tasks.each do |task| %>
    <li><%= link_to task.title, task_path(task)%></li>
    <%= link_to "Delete", task_path(task),
      method: :delete,
      data: { confirm: "Are you sure?" } %>
    <% end %>
    </ul>
    <li> <%= link_to "Create New Task", new_path%></li>
